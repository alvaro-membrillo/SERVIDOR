package org.iesalixar.servidor.services;

import java.util.List;

import org.iesalixar.servidor.model.UserPost;

public interface UserPostService {

	public void insertNewUserPost(final UserPost userPost);

	public void updateUserPost(final UserPost userPost);

	public void deleteUserPost(final UserPost userPost);

	public UserPost searchById(final int commentId, final int UserId);

	public List<UserPost> searchAll();

	public List<UserPost> searchByScore(int puntuacion);

}
