package org.iesalixar.servidor.services;

public class UsuarioServiceImpl implements UsuarioService {
	
	@Autowired
	UsuarioRepository userRepo;
	
	@Override
	public Usuario insertUsuario(Usuario usuario) {
		
		if (usuario!=null) {
			
			return userRepo.save(usuario);
		}
		
		return null;
		
	}
	
}
